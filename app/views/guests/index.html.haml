= link_to "add guest", add_guests_path
%h1 Listing guests

%table
  %tr
    %th Address
    %th Guest
    %th First name
    %th Last name
    %th Title
    %th Email
    %th Twenty one
    %th Eighteen
    %th
    %th
    %th

  - @guests.each do |guest|
    %tr
      %td= guest.address_id
      %td= guest.guest_id
      %td= guest.first_name
      %td= guest.last_name
      %td= guest.title
      %td= guest.email
      %td= guest.twenty_one
      %td= guest.eighteen
      %td= link_to 'Show', guest
      %td= link_to 'Edit', edit_guest_path(guest)
      %td= link_to 'Destroy', guest, :method => :delete, :data => { :confirm => 'Are you sure?' }

%br

